<Matter_Srv_RC
	xmlns="http://elite.com/schemas/transaction/process/write/Matter_Srv_RC">
	<Initialize
		xmlns="http://elite.com/schemas/transaction/object/write/Matter">
		<Add>
			<Matter>
				<Attributes>
					<DisplayName>@DisplayName</DisplayName>
					<Client>2</Client>
					<OriginalClient>@ClientIndex</OriginalClient>
					<MattStatus>@MStatus</MattStatus>
					<MattStatusDate>@MattStatusDate</MattStatusDate>
					<OpenDate>@OpenDate</OpenDate>
					<Narrative>@Narrative</Narrative>
					<Language>1033</Language>
					<TimeTaxCode>@TimeTaxCode</TimeTaxCode>
					<Currency>USD</Currency>
					<LoadSource>@LoadSource</LoadSource>
					<OpenTkpr>@OpenTkpr</OpenTkpr>
					<CurrencyDateList>Work</CurrencyDateList>
					<DateOfOccurence_CCC>@DateOfOccurence_CCC</DateOfOccurence_CCC>
					<IsProspective_CCC>@IsProspective_CCC</IsProspective_CCC>
					<RetainerAmount_CCC>@RetainerAmount_CCC</RetainerAmount_CCC>
					<ScopeOfWork_CCC>@ScopeOfWork_CCC</ScopeOfWork_CCC>
					<!--<ReportingOffice_CCC>@ReportingOffice_CCC</ReportingOffice_CCC>-->
					<IsFixedPrice_CCC>@IsFixedPrice_CCC</IsFixedPrice_CCC>
					<DNENotes_CCC>@DNENotes_CCC</DNENotes_CCC>
					<IsWeb_CCC>@IsWeb_CCC</IsWeb_CCC>
					<EntryDate>@EntryDate</EntryDate>
					<IsEBill>@IsEBill</IsEBill>
					<ElecBillingType>@ElecBillingType</ElecBillingType>
					<ElecNumber>@ElecNumber</ElecNumber>
					<IsConfLetterCAT_CCC>@IsConfLetterCAT_CCC</IsConfLetterCAT_CCC>
					<IsConfLetterEngSvcs_CCC>@IsConfLetterEngSvcs_CCC</IsConfLetterEngSvcs_CCC>
					<IsConfLetterLegal_CCC>@IsConfLetterLegal_CCC</IsConfLetterLegal_CCC>
					<IsConfLetterNatAgmt_CCC>@IsConfLetterNatAgmt_CCC</IsConfLetterNatAgmt_CCC>
					<IsConfLetterRtnr_CCC>@IsConfLetterRtnr_CCC</IsConfLetterRtnr_CCC>
					<IsConfLetterSign_CCC>@IsConfLetterSign_CCC</IsConfLetterSign_CCC>
					<IsConfLetterStd_CCC>@IsConfLetterStd_CCC</IsConfLetterStd_CCC>
					<ReportOfFindings_CCC>@ReportOfFindings_CCC</ReportOfFindings_CCC>
				</Attributes>
				<Children>
					<MattDate>
						@AddMattDate
					</MattDate>
					<!--<MattSite>
						<Add>
							<MattSite>
								<Attributes>
									<Site>8</Site>
									--><!-- Default --><!--
									<MattSiteType>300</MattSiteType>
									--><!-- Default --><!--
								</Attributes>
							</MattSite>
						</Add>
					</MattSite>-->
					<MattRate>
						<Add>
							<MattRate>
								<Attributes>
									<Rate>@MattRate</Rate>
									<!--DNU-->
									<IsActive>1</IsActive>
								</Attributes>
							</MattRate>
						</Add>
					</MattRate>
					<MatterRateExc>
						<Add>
							<MatterRateExc>
								<Attributes>
									<RateExc>@MatterRateExc</RateExc>
									<!-- 561AD9FC-78B8-432B-9816-EE3F864BFD20-->
								</Attributes>
							</MatterRateExc>
						</Add>
					</MatterRateExc>
					@AddMattBudget
					@AddMattFlatFee
					<MattPayor>
						<!-- Payor should be related to the matter client -->
						<Add>
							<MattPayor>
								<Attributes>
									<StartDate>@StartDate</StartDate>
									<CauseNum_CCC>@CauseNum_CCC</CauseNum_CCC>
									<Court_CCC>@Court_CCC</Court_CCC>
									<Representing_CCC>@Representing_CCC</Representing_CCC>
									<Style_CCC>@Style_CCC</Style_CCC>
								</Attributes>
								<Children>
									<MattPayorDetail>
										@AddMattPayorDetail
									</MattPayorDetail>
								</Children>
							</MattPayor>
						</Add>
					</MattPayor>
					<RelatedParties_CCC>
						@AddRelatedParties_CCC
					</RelatedParties_CCC>
					<MatterSpecialInstructions_CCC>
						<Add>
							<MatterSpecialInstructions_CCC>
								<Attributes>
									<Client>@MSClient</Client>

									<!--temporary suppressed until issue with mapping is addressed-->
									<!--<ClientInstr>@ClientInstruction</ClientInstr>-->

									<StartDate>@MSStartDate</StartDate>
									<EndDate>@MSEndDate</EndDate>
									<EffDate>@MSEffDate</EffDate>
									<ReportsTo>@ReportsTo</ReportsTo>
									<AdditionalInstr>@AdditionalInstr</AdditionalInstr>
								</Attributes>
								<Children>
									@AddMatterSpecialInvoiceTo_CCC
								</Children>
							</MatterSpecialInstructions_CCC>
						</Add>
					</MatterSpecialInstructions_CCC>
					<TechRevEngRec_CCC>
						<Add>
							<TechRevEngRec_CCC>
								<Attributes>
									<EngineerOfRecord>@EngineerOfRecord</EngineerOfRecord>
									<Marketer>@Marketer</Marketer>
									<TechincalReviewer>@TechincalReviewer</TechincalReviewer>
								</Attributes>
								<Children>
									@AddCoConsultants_CCC
								</Children>
							</TechRevEngRec_CCC>
						</Add>
					</TechRevEngRec_CCC>
					<IndustryGroupMatter_CCC>
						<Add>
							<IndustryGroupMatter_CCC>
								<Attributes>
									<IndustryGroup>@IndustryGroup</IndustryGroup>
								</Attributes>
							</IndustryGroupMatter_CCC>
						</Add>
					</IndustryGroupMatter_CCC>
					<CommercialInspectionContact_CCC>
						@CommercialInspectionContact_CCC
					</CommercialInspectionContact_CCC>
				</Children>
			</Matter>
		</Add>
	</Initialize>
</Matter_Srv_RC>