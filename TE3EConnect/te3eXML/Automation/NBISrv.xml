<CftWFNewBizIntake_RC
	xmlns="http://elite.com/schemas/transaction/process/write/CftWFNewBizIntake_RC">
  <Initialize
		xmlns="http://elite.com/schemas/transaction/object/write/CftNewBizRequest">
    <Add>
      <CftNewBizRequest>
        <Attributes>
          <Client>@Client</Client>
          <Matter>@Matter</Matter>
          <!-- Matter must be related to the client -->
          <RequestDateTime>@RequestDateTime</RequestDateTime>
          <CftSearchReason>NewPartyExistingMatter</CftSearchReason>
          <!-- NewPartyExistingMatter must be selected to avoid matter creation resolution/popups, but must provide client and matter index -->
          <Description>@Description</Description>
        </Attributes>
        @ChildrenTagXml
      </CftNewBizRequest>
    </Add>
  </Initialize>
</CftWFNewBizIntake_RC>